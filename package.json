{
  "name": "fromgit",
  "description": "Tool to generate project from git repositories",
  "author": "jsoverson@gmail.com",
  "version": "0.0.2",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "bin": "ts-node src/cli.ts",
    "build": "tsc --declaration",
    "compile": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "prepublishOnly": "npm run compile",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "watch": "npm run clean && tsc -w --declaration",
    "test:unit": "mocha",
    "test": "npm run lint && npm run test:unit"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "bin": {
    "fromgit": "./dist/src/cli.js"
  },
  "mocha": {
    "node-option": [
      "experimental-specifier-resolution=node",
      "loader=ts-node/esm"
    ],
    "extends": "etc/.mocharc.json"
  },
  "prettier": "./etc/.prettierrc.json",
  "eslintConfig": {
    "extends": "./etc/.eslintrc.json"
  },
  "type": "module",
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/chai-spies": "^1.0.3",
    "@types/debug": "^4.1.5",
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.1",
    "@types/prompts": "^2.0.14",
    "@types/yargs": "^17.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "eslint": "^7.26.0",
    "mocha": "^9.2.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "ejs": "^3.1.6",
    "fs-extra": "^10.0.0",
    "https-proxy-agent": "^5.0.0",
    "js-yaml": "^4.1.0",
    "prompts": "^2.4.2",
    "tslib": "^2.3.1",
    "yargs": "^17.3.1"
  }
}
